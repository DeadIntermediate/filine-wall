  "scripts": {
    "setup": "node -e \"console.log('Running setup...')\" && npm install && npm run db:push",
    "start:dev": "tsx -r dotenv/config server/index.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "tsx -r dotenv/config server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate:mysql",
    "db:migrate": "drizzle-kit up:mysql",
    "db:studio": "drizzle-kit studio",
    "db:reset": "npm run db:push && echo 'Database reset complete'",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level high",
    "clean": "rimraf dist build node_modules/.cache",
    "healthcheck": "curl -f http://localhost:5000/health || exit 1",
    "prestart": "node -e \"if(!require('fs').existsSync('.env')){console.error('ERROR: .env file not found! Run setup.sh (Linux/Mac) or setup.ps1 (Windows) first.');process.exit(1);}\"",
    "predev": "node -e \"if(!require('fs').existsSync('.env')){console.error('ERROR: .env file not found! Run setup.sh (Linux/Mac) or setup.ps1 (Windows) first.');process.exit(1);}\""
  },
